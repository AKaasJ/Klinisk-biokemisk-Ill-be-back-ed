Workbooks.Open Filename:=MyFile
i = 0

Do While ActiveSheet.Cells(1, i + 2).Value <> ""

    With UserForm1.ComboBox1
        'Lave et for loop der looper igennem cellerne i række 1
        'alle kolonner og adder værdier fra dem som items.
        
        
        
        
        .AddItem ActiveSheet.Cells(1, i + 2).Value & "-" & ActiveSheet.Cells(2, i + 2).Value
    
        
    End With
    i = i + 1
Loop

